<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.lightnovelbackend.dao.ParagraphMapper">
    <select id="getParagraphs" resultType="Paragraph">
        select * from paragraph where book_id=#{book_id} and volume_id=#{volume_id} and chapter_id=#{chapter_id}
    </select>
</mapper>